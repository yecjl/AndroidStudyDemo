<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.study.receivebroadpeople">

    <application android:allowBackup="true" android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name" android:supportsRtl="true" android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <receiver android:name=".ProvinceBroadcastReceiver" >
            <intent-filter android:priority="1000">
                <action android:name="com.study.orderbroadgovernment.GIVEMASHROOM" />
            </intent-filter>
        </receiver>

        <receiver android:name=".CityBroadcastReceiver" >
            <intent-filter android:priority="500">
                <action android:name="com.study.orderbroadgovernment.GIVEMASHROOM" />
            </intent-filter>
        </receiver>

        <receiver android:name=".VillageBroadcastReceiver" >
            <intent-filter android:priority="-1000">
                <action android:name="com.study.orderbroadgovernment.GIVEMASHROOM" />
            </intent-filter>
        </receiver>
    </application>

</manifest>